   "source": [
    "class Solution(object):\n",
    "    def maxProfit(self, prices):\n",
    "\n",
    "        n = len(prices)\n",
    "\n",
    "        maxp = 0\n",
    "        m = float('inf')\n",
    "\n",
    "        for i in range(n):\n",
    "            m = min(m, prices[i])\n",
    "            cp = prices[i] - m\n",
    "            maxp = max(maxp, cp)\n",
    "\n",
    "        return maxp"
   ]
