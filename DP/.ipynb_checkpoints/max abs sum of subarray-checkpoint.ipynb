   "source": [
    "class Solution:\n",
    "    def maxAbsoluteSum(self, nums):\n",
    "\n",
    "        ans = -float('inf')\n",
    "        n = len(nums)\n",
    "\n",
    "        for i in range(n):\n",
    "\n",
    "            cs = 0\n",
    "\n",
    "            for j in range(i, n):\n",
    "\n",
    "                cs += nums[j]\n",
    "\n",
    "                ans = max(ans, abs(cs))\n",
    "        return ans\n",
    "        "
   ]
