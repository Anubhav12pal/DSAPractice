   "source": [
    "class Solution:\n",
    "    def makeTheIntegerZero(self, num1: int, num2: int) -> int:\n",
    "        for k in range(1, 61):\n",
    "            x = num1 - num2 * k\n",
    "            if x < k:\n",
    "                return -1\n",
    "            if k >= x.bit_count():\n",
    "                return k\n",
    "        return -1"
   ]
