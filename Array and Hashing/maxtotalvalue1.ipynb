   "source": [
    "class Solution:\n",
    "    def maxTotalValue(self, nums, k):\n",
    "\n",
    "        # Find the subarray with the max difference and then multiply by k\n",
    "\n",
    "        num1 = max(nums)\n",
    "        num2 = min(nums)\n",
    "\n",
    "        diff = num1 - num2\n",
    "        return diff * k"
   ]
